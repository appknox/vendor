            | {{ finding.title | escape }}
          | {% endif %}
          pre
            code
              | {{ finding.description | escape }}
          | {% endfor %}

      | {% if analysis.vulnerability.non_compliant %}
      section.non-compliant
        div.analysis-type
          a(id="{{ analysis.id }}{{ forloop.counter }}non-compliant")
