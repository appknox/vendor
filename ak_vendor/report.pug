doctype 5
html(lang="en")
  head
    meta(charset="utf-8")
    meta(http-equiv="cache-control" content="max-age=0")
    meta(http-equiv="cache-control" content="no-cache")
    meta(http-equiv="expires" content="0")
    meta(http-equiv="expires" content="Tue, 01 Jan 1980 1:00:00 GMT")
    meta(http-equiv="pragma" content="no-cache")
    link(rel='stylesheet' href="https://unpkg.com/bohemia@0.8.11/dist/report.css")
    style(type='text/css').
      @page {
        @top-left {
          content: "Confidential";
        }
        @top-right {
          content: "{{ date }} UTC";
        }
        @bottom-center {
          border-top: 1px solid #6B6B6B;
          width: 100%;
          content: "Page " counter(page) " of " counter(pages);
        }
      }
  body
    section.new-page
      div.main-logo
        | {% if whitelabel.enabled and whitelabel.logo %}
        | {% if whitelabel.hide_poweredby_logo %}
        div.company-logo
          img(src="{{whitelabel.logo}}")
        | {% else %}
        div.whitelabel-company-logo
          img(src="{{whitelabel.logo}}")
        div.whitelabel-poweredby-logo
          img(src="https://raw.githubusercontent.com/appknox/press-kit/master/logo/poweredby_appknox.png")
        | {% endif %}
        | {% else %}
        div.company-logo
          img(src="https://raw.githubusercontent.com/appknox/press-kit/master/logo/logo-dark-small.png")
        | {% endif %}

      div.main-heading
        | {{file.project.get_platform_display}}
        | {% trans "Security Report" %}
      div.sub-heading
        | {% trans "For Internal Purpose" %}

      div.app-name
        | {% trans "Prepared For" %}: {{file.name}}
      div.doc-description
